<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration>
<!--高 → 低
off
fatal
error
warn
info
debug
trace
all-->
<configuration status="info">

    <Appenders>

        <Console name="CONSOLE">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5p %c{1} -%M   - %m%n"/>
            <!--            <PatternLayout pattern="%d{HH:mm:ss.SSS} %l - %m%n"/>-->
        </Console>

    </Appenders>
    <Loggers>
        <Root level="info">
            <AppenderRef ref="CONSOLE"/>
            <!--            <AppenderRef ref="FILE"/>-->
        </Root>

        <Logger name="com.albert.management" level="debug"/>
    </Loggers>

</configuration>
